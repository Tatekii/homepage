{
  "name": "nextjs",
  "version": "0.3.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "dev:no-optimize": "SKIP_IMAGE_OPTIMIZATION=true next dev",
    "dev:debug": "NODE_ENV=development DEBUG_IMAGE_OPTIMIZATION=true next dev",
    "dev:no-cache": "NODE_ENV=development DISABLE_IMAGE_CACHE=true next dev",
    "build": "NEXT_OPTIMIZE_FONTS=true NEXT_MINIMIZE_ASSETS=true next build",
    "build:responsive": "GENERATE_RESPONSIVE=true NEXT_OPTIMIZE_FONTS=true NEXT_MINIMIZE_ASSETS=true next build",
    "build:debug": "DEBUG_IMAGE_OPTIMIZATION=true NEXT_OPTIMIZE_FONTS=true NEXT_MINIMIZE_ASSETS=true next build",
    "build:no-cache": "DISABLE_IMAGE_CACHE=true NEXT_OPTIMIZE_FONTS=true NEXT_MINIMIZE_ASSETS=true next build", 
    "build:analyze": "node analyze-bundle.js",
    "build:no-optimize": "SKIP_IMAGE_OPTIMIZATION=true next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "analyze:images": "chmod +x scripts/analyze-images.sh && ./scripts/analyze-images.sh",
    "analyze:images:dev": "chmod +x scripts/analyze-images.sh && ./scripts/analyze-images.sh --dev",
    "analyze:images:legacy": "chmod +x scripts/check-image-sizes.sh && ./scripts/check-image-sizes.sh",
    "optimize:images:legacy": "node scripts/image-optimizer.js",
    "optimize:images:full:legacy": "node scripts/image-optimizer.js --generate-responsive",
    "optimize:images:dev:legacy": "node scripts/image-optimizer.js --dev",
    "clear:image-cache": "chmod +x scripts/clear-image-cache.sh && ./scripts/clear-image-cache.sh",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch"
  },
  "dependencies": {
